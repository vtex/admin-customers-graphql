{
  "name": "admin-customers-graphql",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "graphql-markdown": "node graphql-markdown-generator.js",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@vtex/prettier-config": "^0.3.1",
    "eslint": "^6.8.0",
    "eslint-config-vtex": "^12.7.0",
    "graphql": "^15.0.0",
    "graphql-markdown": "^6.0.0",
    "graphql-schema-utilities": "^1.1.3",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "minimist": "^1.2.6",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  }
}
